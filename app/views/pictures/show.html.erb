<%= link_to("Back to Pictures", "/pictures") %> | <% if current_user.id == @picture.user_id %><%= link_to("Edit", "/pictures/#{@picture.id}/edit") %> | <%= link_to("Delete", "/pictures/#{@picture.id}", method: :delete, data: {confirm: "Are you sure you want to delete this picture?"})  %> <% end %>

<p><%= @picture.title %>, <%= @picture.artist %></p>
